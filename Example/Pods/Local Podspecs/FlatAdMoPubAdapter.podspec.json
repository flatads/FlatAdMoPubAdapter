{
  "name": "FlatAdMoPubAdapter",
  "version": "0.1.0",
  "summary": "FlatAds_sdk MoPub Adapter",
  "description": "An FlatAds_sdk adapter 4 mopub.",
  "homepage": "https://github.com/flatads/FlatAdMoPubAdapter",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "flatads": "chenwh02@flatincbr.com"
  },
  "source": {
    "git": "https://github.com/flatads/FlatAdMoPubAdapter.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "source_files": "FlatAdMoPubAdapter/Classes/**/*",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "dependencies": {
    "FlatAds_sdk": [
      "~> 1.0.5"
    ],
    "mopub-ios-sdk": [
      "~> 5.17.0"
    ]
  },
  "subspecs": [
    {
      "name": "Banner",
      "source_files": "iOSMoPubAdapter/Banner/*.{h,m}"
    },
    {
      "name": "Configuration",
      "source_files": "iOSMoPubAdapter/Configuration/*.{h,m}"
    },
    {
      "name": "Interstitial",
      "source_files": "iOSMoPubAdapter/Interstitial/*.{h,m}"
    },
    {
      "name": "Native",
      "source_files": "iOSMoPubAdapter/Native/*.{h,m}"
    },
    {
      "name": "Rewarded",
      "source_files": "iOSMoPubAdapter/Rewarded/*.{h,m}"
    },
    {
      "name": "Util",
      "source_files": "iOSMoPubAdapter/Util/*.{h,m}"
    }
  ]
}
